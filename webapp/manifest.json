{
	"_version": "1.5.0",
	"sap.app": {
		"id": "gdprcockpit",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZGDPR_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.2masterdetail",
			"version": "1.44.7"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"handleValidation": true,
		"rootView": {
			"viewName": "gdprcockpit.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.36.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.36.0"
				},
				"sap.m": {
					"minVersion": "1.36.0"
				},
				"sap.ui.layout": {
					"minVersion": "1.36.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "gdprcockpit.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"metadataUrlParams": {
						"sap-documentation": "heading"
					},
					"defaultBindingMode": "TwoWay"
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "gdprcockpit.view",
				"controlId": "idAppControl",
				"controlAggregation": "detailPages",
				"bypassed": {
					"target": [
						"master",
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "master",
					"target": [
						"master",
						"splash"
					]
				},
				{
					"pattern": "datafields",
					"name": "FIELD",
					"target": [
						"master",
						"datafields"
					]
				},
				{
					"pattern": "datafields/{id}",
					"name": "fieldDetail",
					"target": [
						"master",
						"fieldDetail"
					]
				},
				{
					"pattern": "datasources/:id:",
					"name": "SOURCE",
					"target": [
						"master",
						"datasources"
					]
				},
				{
					"pattern": "datavault",
					"name": "VAULT",
					"target": [
						"master",
						"datavault"
					]
				},
				{
					"pattern": "datavault/{id}",
					"name": "datavaultentity",
					"target": [
						"master",
						"vaultentity"
					]
				},
				{
					"pattern": "descriptions/{objectType}",
					"name": "descriptions",
					"target": [
						"master",
						"maintaindescriptions"
					]
				},
				{
					"pattern": "dsr",
					"name": "DSR",
					"target": [
						"master",
						"dsrlist"
					]
				},
				{
					"pattern": "dsr/{dsrId}",
					"name": "dsrdetail",
					"target": [
						"master",
						"dsrdetail"
					]
				}
			],
			"targets": {
				"master": {
					"viewName": "Master",
					"viewLevel": 1,
					"viewId": "master",
					"controlAggregation": "masterPages"
				},
				"splash": {
					"viewName": "Splash",
					"viewLevel": 2,
					"viewId": "splash"
				},
				"datafields": {
					"viewName": "DataFields",
					"viewLevel": 2,
					"viewId": "datafields"
				},
				"fieldDetail": {
					"viewName": "DataFieldDetail",
					"viewLevel": 3,
					"viewId": "datafielddetail"
				},
				"datasources": {
					"viewName": "DataSources",
					"viewLevel": 2,
					"viewId": "datasources"
				},
				"datavault": {
					"viewName": "DataVault",
					"viewLevel": 2,
					"viewId": "datavault"
				},
				"vaultentity": {
					"viewName": "DataVaultEntity",
					"viewLevel": 2,
					"viewId": "vaultentity"
				},
				"maintaindescriptions": {
					"viewName": "MaintainDescriptions",
					"viewId": "maintainDescriptions"
				},
				"dsrlist": {
					"viewName": "DsrList",
					"viewLevel": 2,
					"viewId": "dsrlist"
				},
				"dsrdetail": {
					"viewName": "Dsr",
					"viewLevel": 3,
					"viewId": "dsr"
				},
				"detailObjectNotFound": {
					"viewName": "DetailObjectNotFound",
					"viewId": "detailObjectNotFound"
				},
				"detailNoObjectsAvailable": {
					"viewName": "DetailNoObjectsAvailable",
					"viewId": "detailNoObjectsAvailable"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zgdprcp/webapp",
		"_version": "1.1.0"
	}
}